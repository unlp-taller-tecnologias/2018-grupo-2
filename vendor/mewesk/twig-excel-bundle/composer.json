{
    "name": "mewesk/twig-excel-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony bundle provides a PhpExcel integration for Twig",
    "keywords": ["excel","phpexcel","twig","symfony"],
    "homepage": "https://github.com/MewesK/TwigExcelBundle",
    "license": "MIT",
    "authors": [
        { "name": "Mewes Kochheim", "email": "mewes@kochheim.de" }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "~2.7|~3.0",
        "phpoffice/phpexcel": "~1.8.1"
    },
    "require-dev": {
        "symfony/symfony": "~2.7|~3.0",
        "symfony/phpunit-bridge": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "mpdf/mpdf": "~6.0"
    },
    "suggest": {
        "mpdf/mpdf": "~6.0"
    },
    "autoload": {
        "psr-4": { "MewesK\\TwigExcelBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
